package functions_Methods;

public class swap {
    public static void main(String[] args) {
        int a= 10, b=40, temp;
        // swap the numbers
       // temp = a;
        //a = b;
        //b= temp;
        swap(a,b);
        System.out.println(a+ " " +b);
    }
    static void swap(int a , int b){
        int temp;

        // swap the numbers
        temp = a;
        a = b;
        b= temp;
    }
}
